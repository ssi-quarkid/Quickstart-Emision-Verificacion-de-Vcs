#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@extrimian/vc-verifier/node_modules/@mattrglobal/bls12381-key-pair/node_modules/neon-cli/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../@extrimian/vc-verifier/node_modules/@mattrglobal/bls12381-key-pair/node_modules/neon-cli/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
